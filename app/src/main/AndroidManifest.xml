<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.domo.network.artofdevelopment"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".part1_activity_launcher.Part1Activity"/>
        <activity
            android:name=".part1_activity_launcher.SingleInstanceActivity"
            android:launchMode="singleInstance"/>
        <activity
            android:name=".part1_activity_launcher.SingleTaskActivity"
            android:launchMode="singleTask"/>
        <activity
            android:name=".part1_activity_launcher.SingleTopActivity"
            android:launchMode="singleTop"/>
        <activity
            android:name=".part1_activity_launcher.StandardActivity"
            android:launchMode="standard"/>
    </application>

</manifest>